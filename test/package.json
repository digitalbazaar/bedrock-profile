{
  "name": "bedrock-profile-test",
  "version": "0.0.1-0",
  "type": "module",
  "description": "Bedrock Profile test",
  "private": true,
  "scripts": {
    "test": "node --preserve-symlinks test.js test",
    "debug": "node --preserve-symlinks test.js test --log-level debug",
    "coverage": "cross-env NODE_ENV=test c8 npm test",
    "coverage-ci": "cross-env NODE_ENV=test c8 --reporter=lcovonly --reporter=text-summary --reporter=text npm test",
    "coverage-report": "c8 report"
  },
  "dependencies": {
    "@bedrock/account": "^8.0.0",
    "@bedrock/app-identity": "^4.0.0",
    "@bedrock/core": "^6.0.1",
    "@bedrock/did-context": "^4.0.0",
    "@bedrock/did-io": "^9.0.1",
    "@bedrock/edv-storage": "github:digitalbazaar/bedrock-edv-storage#update-dependencies",
    "@bedrock/express": "^8.0.0",
    "@bedrock/https-agent": "^4.0.0",
    "@bedrock/jsonld-document-loader": "^3.0.0",
    "@bedrock/kms": "github:digitalbazaar/bedrock-kms#update-dependencies",
    "@bedrock/kms-http": "github:digitalbazaar/bedrock-kms-http#update-dependencies",
    "@bedrock/ledger-context": "^23.0.0",
    "@bedrock/meter": "^4.0.0",
    "@bedrock/meter-http": "^9.0.1",
    "@bedrock/meter-usage-reporter": "^8.0.0",
    "@bedrock/mongodb": "^10.0.0",
    "@bedrock/package-manager": "^3.0.0",
    "@bedrock/passport": "^10.0.0",
    "@bedrock/profile": "file:..",
    "@bedrock/security-context": "^7.0.0",
    "@bedrock/server": "^5.0.0",
    "@bedrock/ssm-mongodb": "github:digitalbazaar/bedrock-ssm-mongodb#update-dependencies",
    "@bedrock/test": "^8.0.5",
    "@bedrock/validation": "^7.0.0",
    "@bedrock/veres-one-context": "^14.0.1",
    "@bedrock/zcap-storage": "^8.0.0",
    "@digitalbazaar/ed25519-signature-2020": "^4.0.1",
    "@digitalbazaar/ezcap": "^3.0.1",
    "@digitalbazaar/zcap": "^8.0.0",
    "c8": "^7.11.3",
    "cross-env": "^7.0.3",
    "uuid": "^8.3.2"
  },
  "c8": {
    "excludeNodeModules": false,
    "include": [
      "node_modules/@bedrock/profile/**"
    ],
    "exclude": [
      "node_modules/@bedrock/profile/node_modules/**"
    ],
    "reporter": [
      "lcov",
      "text-summary",
      "text"
    ]
  }
}
